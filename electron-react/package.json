  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "npm-run-all -p dev:react dev:electron",
    "dev:react": "vite",
    "dev:electron": "npm run transpile:electron && cross-env NODE_DEV=development electron .",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "transpile:electron": "tsc --project src/electron/tsconfig.json",
    "dist:win": "npm run dev:electron & npm run build & electron-builder --win --x64",
    "dist:linux": "npm run dev:electron & npm run build & electron-builder --linux --x64",
    "dist:mac": "npm run dev:electron & npm run build & electron-builder --mac --x64"
  },
